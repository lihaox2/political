  <insert id="insertSelective" parameterType="com.bayee.political.domain.PoliceStation" >
    insert into police_station
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="policeStationName != null" >
        police_station_name,
